{
  "name": "novo-lib",
  "license": "MIT",
  "version": "0.0.9-development",
  "private": false,
  "description": "React Typescript Library Boilerplate",
  "author": "Novo",
  "homepage": "https://github.com/AleksBeliun/novo-lib",
  "bugs": {
    "url": "https://github.com/AleksBeliun/novo-lib/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AleksBeliun/novo-lib.git"
  },
  "scripts": {
    "build": "yarn clean; rollup -c",
    "clean": "rimraf dist",
    "dev": "yarn storybook",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,js,jsx}\"",
    "format:check": "prettier --list-different \"src/**/*.{ts,tsx,json,js,jsx}\"",
    "lint": "yarn lint:script && yarn lint:style",
    "lint:script": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "lint:style": "stylelint ./src",
    "storybook": "start-storybook -p 9001 -s ./src/assets -c .storybook",
    "test": "jest --maxWorkers=2",
    "test:watch": "yarn test --watch",
    "test:coverage": "jest --coverage --colors --maxWorkers=2",
    "hygen": "hygen",
    "component:new": "hygen component with-prompt",
    "publish": "npx semantic-release --no-ci",
    "semantic-release": "semantic-release"
  },
  "main": "index.js",
  "module": "index.esm.js",
  "types": "index.d.ts",
  "dependencies": {
    "postcss": "^8.3.11"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^8.0.3",
    "@semantic-release/release-notes-generator": "^10.0.2",
    "@storybook/react": "^6.3.12",
    "@testing-library/react": "^11.2.2",
    "@types/classnames": "^2.3.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^12.20.36",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "babel-loader": "^8.2.3",
    "classnames": "^2.3.1",
    "conventional-changelog-conventionalcommits": "^4.6.1",
    "css-loader": "^5.2.7",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.0",
    "hygen": "^6.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "node-sass": "^5.0.0",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.59.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "sass-loader": "^10.1.0",
    "semantic-release": "^18.0.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.21.0",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.11",
    "typescript": "^3.8.3",
    "webpack": "^4.46.0"
  },
  "engines": {
    "node": ">=12.0.0",
    "yarn": "^1.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "branches": [
      "master",
      "next"
    ],
    "preset": "conventionalcommits",
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "releaseRules": [
            {
              "type": "revert",
              "release": "patch"
            },
            {
              "type": "build",
              "release": "patch"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogTitle": "# Changelog"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "dist"
        }
      ],
      [
        "@semantic-release/git",
        {
          "message": "chore(release): ${nextRelease.version} [skip ci]",
          "assets": [
            "CHANGELOG.md"
          ]
        }
      ]
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
